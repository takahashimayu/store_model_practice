<h1>Showing interview</h1>

<p style="color: green"><%= notice %></p>

<%= render @interview %>
<div>
  <%= link_to "Edit this interview", edit_interview_path(@interview), class: 'btn btn-primary' %>
  <%= link_to "Back to interviews", interviews_path, class: 'btn btn-primary' %>

  <%= button_to "Destroy this interview", @interview, method: :delete, class: 'btn btn-danger mt-1' %>
</div>
